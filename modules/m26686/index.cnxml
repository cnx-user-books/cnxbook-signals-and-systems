                        </m:mrow>
                      </m:mfrac>
                      <m:mo stretchy="false">−</m:mo>
                      <m:mfrac>
                        <m:mn>2</m:mn>
                        <m:mrow>
                          <m:mi>s</m:mi>
                          <m:mo stretchy="false">+</m:mo>
