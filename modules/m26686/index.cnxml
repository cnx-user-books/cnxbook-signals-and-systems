                        <m:mn>3s</m:mn>
                      </m:mrow>
                      <m:mo stretchy="false">+</m:mo>
                      <m:mn>2</m:mn>
                    </m:mrow>
                  </m:mfrac>
                </m:mrow>
              </m:mrow>
            </m:mstyle>
            <m:mrow/>
          </m:mrow>
          <m:annotation encoding="StarMath 5.0"> size 12{ {  {s rSup { size 8{2} } }  over  {s rSup { size 8{2} } +3s+2} } = {  { \( s rSup { size 8{2} } +3s+2 \)  -  \( 3s+2 \) }  over  {s rSup { size 8{2} } +3s+2} } } {}</m:annotation>
