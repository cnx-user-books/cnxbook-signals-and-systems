                      </m:mtd>
                      <m:mtd>
                        <m:mrow/>
                      </m:mtd>
                    </m:mtr>
                  </m:mtable>
                  <m:mstyle fontstyle="italic">
                    <m:mrow>
                      <m:mtext>and</m:mtext>
                    </m:mrow>
                  </m:mstyle>
                  <m:mtable>
                    <m:mtr>
                      <m:mtd>
                        <m:mrow/>
                      </m:mtd>
                      <m:mtd>
                        <m:mrow/>
                      </m:mtd>
                    </m:mtr>
                  </m:mtable>
                  <m:mrow>
                    <m:mrow>
                      <m:munder>
                        <m:mo stretchy="false">∑</m:mo>
                        <m:mstyle fontsize="8pt">
                          <m:mrow>
