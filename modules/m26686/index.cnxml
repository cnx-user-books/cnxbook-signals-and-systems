                        <m:mrow>
                          <m:mi>L</m:mi>
                        </m:mrow>
                      </m:mstyle>
                    </m:mover>
                    <m:mtable>
                      <m:mtr>
                        <m:mtd>
                          <m:mrow/>
                        </m:mtd>
                        <m:mtd>
                          <m:mrow/>
                        </m:mtd>
                      </m:mtr>
                    </m:mtable>
