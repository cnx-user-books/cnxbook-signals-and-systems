                    <m:mrow>
                      <m:mrow>
                        <m:msup>
                          <m:mi>s</m:mi>
                          <m:mstyle fontsize="8pt">
                            <m:mrow>
                              <m:mn>2</m:mn>
                            </m:mrow>
                          </m:mstyle>
                        </m:msup>
