                          <m:mi>t</m:mi>
                          <m:mo stretchy="false">)</m:mo>
                          <m:mtable>
                            <m:mtr>
                              <m:mtd>
                                <m:mrow/>
                              </m:mtd>
                              <m:mtd>
                                <m:mrow/>
                              </m:mtd>
                            </m:mtr>
                          </m:mtable>
                          <m:mover>
                            <m:mo stretchy="false">⇔</m:mo>
                            <m:mstyle fontsize="8pt">
                              <m:mrow>
                                <m:mi>L</m:mi>
                              </m:mrow>
                            </m:mstyle>
                          </m:mover>
                          <m:mtable>
                            <m:mtr>
                              <m:mtd>
                                <m:mrow/>
                              </m:mtd>
                              <m:mtd>
                                <m:mrow/>
                              </m:mtd>
                            </m:mtr>
                          </m:mtable>
                          <m:mi>X</m:mi>
                          <m:mo stretchy="false">(</m:mo>
                          <m:mi>s</m:mi>
                          <m:mo stretchy="false">)</m:mo>
                        </m:mrow>
                      </m:mrow>
                    </m:mstyle>
                    <m:mrow/>
                  </m:mrow>
                </m:mtr>
                <m:mtr>
                  <m:mrow>
                    <m:mi>x</m:mi>
