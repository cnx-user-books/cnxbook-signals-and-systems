          <m:annotation encoding="StarMath 5.0"> size 12{V rSub { size 8{o} }  \( s \) =H \( s \) = {  {s rSup { size 8{2} } }  over  { \( s+1 \)  \( s+2 \) } } = {  {s rSup { size 8{2} } }  over  {s rSup { size 8{2} } +3s+2} } } {}</m:annotation>
        </m:semantics>
      </m:math>
    </para>
    <para id="id8246085">as follows</para>
    <para id="id8246089">
      <m:math>
        <m:semantics>
          <m:mrow>
            <m:mstyle fontsize="12pt">
              <m:mrow>
                <m:mrow>
                  <m:mfrac>
                    <m:msup>
                      <m:mi>s</m:mi>
                      <m:mstyle fontsize="8pt">
                        <m:mrow>
                          <m:mn>2</m:mn>
                        </m:mrow>
                      </m:mstyle>
                    </m:msup>
                    <m:mrow>
                      <m:mrow>
                        <m:msup>
                          <m:mi>s</m:mi>
                          <m:mstyle fontsize="8pt">
                            <m:mrow>
                              <m:mn>2</m:mn>
                            </m:mrow>
                          </m:mstyle>
                        </m:msup>
                        <m:mo stretchy="false">+</m:mo>
                        <m:mn>3s</m:mn>
