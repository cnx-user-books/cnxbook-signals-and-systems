                  </m:mrow>
                  <m:mo stretchy="false">(</m:mo>
                  <m:mi>s</m:mi>
                  <m:mo stretchy="false">)</m:mo>
                  <m:mrow>
                    <m:mi>X</m:mi>
                    <m:mo stretchy="false">=</m:mo>
                    <m:mfrac>
                      <m:mrow>
                        <m:munderover>
                          <m:mo stretchy="false">∑</m:mo>
                          <m:mstyle fontsize="8pt">
                            <m:mrow>
                              <m:mrow>
                                <m:mi>m</m:mi>
                                <m:mo stretchy="false">=</m:mo>
