                    </m:mfrac>
                  </m:mrow>
                  <m:mi>X</m:mi>
                </m:mrow>
              </m:mrow>
            </m:mstyle>
            <m:mrow/>
          </m:mrow>
          <m:annotation encoding="StarMath 5.0"> size 12{Y=H \( s \) X= {  { Sum cSub { size 8{m=0} }  cSup { size 8{M} }  {b rSub { size 8{m} } s rSup { size 8{m} } } }  over  { Sum cSub { size 8{n=0} }  cSup { size 8{N} }  {b rSub { size 8{n} } s rSup { size 8{n} } } } } X} {}</m:annotation>
        </m:semantics>
      </m:math>
    </para>
    <para id="id10840925">
      <m:math>
        <m:semantics>
          <m:mrow>
            <m:mstyle fontsize="12pt">
              <m:mrow>
                <m:mrow>
                  <m:mi>H</m:mi>
                  <m:mo stretchy="false">(</m:mo>
                  <m:mi>s</m:mi>
                  <m:mrow>
                    <m:mrow>
                      <m:mo stretchy="false">)</m:mo>
