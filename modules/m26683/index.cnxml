                          <m:mstyle fontstyle="italic">
                            <m:mrow>
                              <m:mtext>RC</m:mtext>
                            </m:mrow>
                          </m:mstyle>
                        </m:mfrac>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>s</m:mi>
                        <m:mo stretchy="false">+</m:mo>
                        <m:mfrac>
                          <m:mn>1</m:mn>
                          <m:mstyle fontstyle="italic">
                            <m:mrow>
                              <m:mtext>RC</m:mtext>
                            </m:mrow>
                          </m:mstyle>
                        </m:mfrac>
                      </m:mrow>
                    </m:mrow>
                  </m:mfrac>
                  <m:mo stretchy="false">)</m:mo>
                </m:mrow>
              </m:mrow>
            </m:mstyle>
            <m:mrow/>
          </m:mrow>
          <m:annotation encoding="StarMath 5.0"> size 12{H \( s \) = {  {V}  over  {I} } = {  {1}  over  {C} }  \(  {  {s}  over  {s rSup { size 8{2} } + {  {1}  over  { ital "RC"} } s+ {  {1}  over  { ital "RC"} } } }  \) } {}</m:annotation>
        </m:semantics>
      </m:math>
    </para>
    <figure id="id12360088">
