                    </m:mfenced>
                  </m:mrow>
                  <m:msub>
                    <m:mi>v</m:mi>
                    <m:mstyle fontsize="8pt">
                      <m:mrow>
                        <m:mi>o</m:mi>
                      </m:mrow>
                    </m:mstyle>
                  </m:msub>
                  <m:mo stretchy="false">[</m:mo>
                  <m:mi>n</m:mi>
                  <m:mrow>
                    <m:mo stretchy="false">]</m:mo>
                    <m:mo stretchy="false">+</m:mo>
                    <m:mfenced open="[" close="]">
                      <m:mfrac>
                        <m:mi>α</m:mi>
                        <m:mrow>
                          <m:mn>1</m:mn>
                          <m:mo stretchy="false">+</m:mo>
                          <m:mi>α</m:mi>
                        </m:mrow>
                      </m:mfrac>
                    </m:mfenced>
                  </m:mrow>
                  <m:msub>
                    <m:mi>v</m:mi>
                    <m:mstyle fontsize="8pt">
                      <m:mrow>
                        <m:mi>i</m:mi>
                      </m:mrow>
