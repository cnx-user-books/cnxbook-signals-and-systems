                  <m:mn>0</m:mn>
                </m:mrow>
              </m:mrow>
            </m:mstyle>
            <m:mrow/>
          </m:mrow>
          <m:annotation encoding="StarMath 5.0"> size 12{s rSup { size 8{2} } + {  {1}  over  { ital "LC"} } =0} {}</m:annotation>
        </m:semantics>
      </m:math>
    </para>
    <para id="id6155628">so that the natural frequencies are</para>
    <para id="id6155632">
      <m:math>
        <m:semantics>
          <m:mrow>
            <m:mstyle fontsize="12pt">
              <m:mrow>
                <m:mrow>
                  <m:mrow>
                    <m:msub>
                      <m:mi>s</m:mi>
                      <m:mstyle fontsize="8pt">
                        <m:mrow>
