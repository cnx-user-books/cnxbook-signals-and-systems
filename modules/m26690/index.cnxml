                  </m:mrow>
                  <m:mo stretchy="false">=</m:mo>
                  <m:mfrac>
                    <m:mn>1</m:mn>
                    <m:mrow>
                      <m:mrow>
                        <m:mn>1</m:mn>
                        <m:mo stretchy="false">+</m:mo>
                        <m:mrow>
                          <m:mi>T</m:mi>
                          <m:mo stretchy="false">/</m:mo>
                          <m:mo stretchy="false">(</m:mo>
                        </m:mrow>
                      </m:mrow>
