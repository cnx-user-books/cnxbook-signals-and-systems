                      <m:mrow>
                        <m:mrow>
                          <m:mn>1</m:mn>
                          <m:mo stretchy="false">−</m:mo>
                          <m:mo stretchy="false">(</m:mo>
                        </m:mrow>
                        <m:mrow>
                          <m:mn>2</m:mn>
                          <m:mo stretchy="false">+</m:mo>
                          <m:mi>α</m:mi>
                        </m:mrow>
                        <m:mo stretchy="false">)</m:mo>
                        <m:mrow>
