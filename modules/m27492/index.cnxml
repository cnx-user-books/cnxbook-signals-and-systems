            </m:mstyle>
            <m:mrow/>
          </m:mrow>
          <m:annotation encoding="StarMath 5.0"> size 12{x \( t \) = ital "ax" rSub { size 8{1} }  \( t \) + ital "bx" rSub { size 8{2} }  \( t \) } {}</m:annotation>
        </m:semantics>
