                            <m:mo stretchy="false">∣</m:mo>
                            <m:mo stretchy="false">=</m:mo>
                            <m:mtext>20</m:mtext>
                          </m:mrow>
                          <m:msub>
                            <m:mtext>log</m:mtext>
                            <m:mstyle fontsize="8pt">
                              <m:mrow>
                                <m:mtext>10</m:mtext>
                              </m:mrow>
                            </m:mstyle>
                          </m:msub>
                          <m:mo stretchy="false">∣</m:mo>
                          <m:mi>K</m:mi>
                          <m:mrow>
                            <m:mo stretchy="false">∣</m:mo>
                            <m:mo stretchy="false">+</m:mo>
                            <m:mtext>20</m:mtext>
                          </m:mrow>
                          <m:msub>
                            <m:mtext>log</m:mtext>
                            <m:mstyle fontsize="8pt">
                              <m:mrow>
                                <m:mtext>10</m:mtext>
                              </m:mrow>
                            </m:mstyle>
                          </m:msub>
                          <m:mo stretchy="false">∣</m:mo>
                          <m:mi>j</m:mi>
                          <m:mrow>
                            <m:mstyle fontstyle="italic">
                              <m:mrow>
